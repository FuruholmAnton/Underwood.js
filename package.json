    "laravel-mix": "^4.0.6",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "vue-template-compiler": "2.6.9"
  }
